/dts-v1/;

#include "ns3z.dtsi"

/ {
	model = "NS3z Combo SVK (BCM958733k)";

	aliases {
		serial0 = &uart1;
		serial1 = &uart0;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};
};

&uart1 {
	status = "okay";
};

&ssp0 {
	cs-gpios = <&gpio_hsls 34 0>;
	status = "okay";
};

&ssp1 {
	cs-gpios = <&gpio_hsls 96 0>;
	status = "okay";
};

&sdio0 {
	non-removable;
	full-pwr-cycle;
	status = "okay";
};

&sdio1 {
	full-pwr-cycle;
	status = "okay";
};
