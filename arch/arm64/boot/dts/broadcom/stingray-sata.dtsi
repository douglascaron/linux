				phy-names = "sata-phy";
			};
		};

		sata_phy19: sata_phy@68132100 {
			compatible = "brcm,iproc-sr-sata-phy";
			reg = <0x68132100 0x1000>;
			reg-names = "phy";
			#address-cells = <1>;
			#size-cells = <0>;

			sata19_phy0: sata-phy@0 {
				reg = <0>;
				#phy-cells = <0>;
#if (NUM_SATA > 19)
				status = "okay";
#else
				status = "disabled";
#endif
			};
		};
#if (NUM_SATA > 20)
#error "NUM_SATA > 20"
